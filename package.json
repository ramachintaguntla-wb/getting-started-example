{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^6.0.1",
    "semantic-release": "^18.0.1",
    "semantic-release-monorepo": "^7.0.5"
  },
  "scripts": {
    "release": "lerna exec --concurrency 1 -- npx --no-install semantic-release -e semantic-release-monorepo ",
    "install": "lerna exec --concurrency 1 -- npm install"
  },
  "plugins": [    
"@semantic-release/commit-analyzer",    
"@semantic-release/release-notes-generator",   
 "@semantic-release/changelog",    
    { "changelogFile": "CHANGELOG.md"    },    
 "@semantic-release/npm",    
 ["@semantic-release/git",      
 {"assets": ["CHANGELOG.md"]      
 }    
 ]  
 ],  
 "release": { 
    "branches": ["master", "testlernanew"],    
    "repositoryUrl": "https://github.com/ramachintaguntla-wb/getting-started-example",    
    "verifyConditions": ["@semantic-release/github"],    
    "publish": "@semantic-release/github",    
    "fail": false,    
    "success": false  
    }
}
